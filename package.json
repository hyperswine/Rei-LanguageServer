{
  "name": "rei",
  "displayName": "Rei",
  "description": "Syntax highlighting for .rei files and the prei build system",
  "version": "0.1.8",
  "engines": {
    "vscode": "^1.73.0"
  },
  "icon": "icons/ReiLogo.png",
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "rei",
        "aliases": [
          "Rei",
          "rei"
        ],
        "extensions": [
          ".rei"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/Rei.png",
          "dark": "./icons/Rei-Light.png"
        }
      },
      {
        "id": "phantasm",
        "aliases": [
          "Phantasm",
          "phantasm",
          "pasm"
        ],
        "extensions": [
          ".asm",
          ".pasm"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icons/Phantasm-Light.png",
          "dark": "./icons/Phantasm-Dark.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "rei",
        "scopeName": "source.rei",
        "path": "./syntaxes/rei.json"
      },
      {
        "language": "phantasm",
        "scopeName": "source.pasm",
        "path": "./syntaxes/phantasm.json"
      }
    ],
    "commands": [
      {
        "title": "Enable CodeLens",
        "command": "rei.enableCodeLens",
        "category": "CodeLens Sample"
      },
      {
        "title": "Disable Codelens",
        "command": "rei.disableCodeLens",
        "category": "CodeLens Sample"
      }
    ],
    "configuration": {
      "properties": {
        "rei.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  },
  "repository": {
    "url": "https://github.com/hyperswine/rei-lsp"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./"
  },
  "publisher": "REILANGUAGE",
  "dependencies": {
    "@types/vscode": "^1.86.0",
    "typescript": "^5.3.3"
  }
}